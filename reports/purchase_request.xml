<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="report_purchase_request_card">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <div class="page">
                        <div class="row">
                            <div class="col-xs-2">
                                Name : <t t-esc="doc.name"/>
                            </div>
                            <div class="col-xs-8">
                                <h1>test</h1>
                                Request_by : <t t-esc="doc.request_by"/>
                                Check_by :<t t-esc="doc.check_by"/>
                                Department : <t t-esc="doc.department"/>
                                Cost total :<t t-esc="doc.cost_total"/>
                            </div>
                        </div>
                    </div>
                </t>
            </t>
        </template>
        <template id="report_purchase_request">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="purchase_request.report_purchase_request_card"/>
                </t>
            </t>
        </template>
    </data>
</odoo>